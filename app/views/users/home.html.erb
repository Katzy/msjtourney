
<div class="team-name">
  <h3><%= @user.school.upcase %></h3>
</div>
<div class="container-team" ng-view="">
  <div class="row">
    <div class="panel">
      <div class="panel-body">
        <div class="table-responsive">
          <table class="table table-bordered table-hover" id="teams_table">
            <thead>
              <tr>
                <th class="col-md-1">Team Name</th>
                <th class="col-md-1">Section</th>
                <th class="col-md-1">Coach</th>
                <th class="col-md-1">Cell</th>
                <th class="col-md-1">Email</th>
                <th class="col-md-1">Edit Team Info</th>

              </tr>
            </thead>
            <tbody>
                <%= render partial: '/users/table_row', locals: { user: @user} %>
            </tbody>
          </table>
        </div>
      </div>
    </div>
  </div>
</div>

<%= render partial: 'users/wrestlers/index', locals: { wrestlers: @wrestlers} %>
<br>
